{
	"name": "Xiami Exporter",
	"version": "1.0",
	"manifest_version": 2,
	"icons": {
		"128": "img/browse.gif",
		"48": "img/browse.gif",
		"16": "img/browse.gif"
	},
	"description": "虾米收藏导出",
	"permissions": ["tabs",
	"http://www.xiami.com/",
	"notifications"],
	"options_page": "options.html",
	"background": {
		"scripts": []
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "img/browse.gif"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' http://www.xiami.com; object-src 'self'"
}